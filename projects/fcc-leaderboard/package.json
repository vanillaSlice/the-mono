{
  "name": "fcc-leaderboard",
  "version": "1.1.0",
  "description": "A FreeCodeCamp leaderboard built using React",
  "keywords": [
    "freecodecamp",
    "leaderboard",
    "react"
  ],
  "homepage": "https://fccleaderboard.mikelowe.xyz/",
  "bugs": {
    "url": "https://github.com/vanillaSlice/FCCLeaderboard/issues",
    "email": "mikelowedev@gmail.com"
  },
  "license": "MIT",
  "author": {
    "name": "Mike Lowe",
    "email": "mikelowedev@gmail.com",
    "url": "https://www.mikelowe.xyz/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vanillaSlice/FCCLeaderboard.git"
  },
  "scripts": {
    "test": "run-s test:*",
    "test:lint": "run-s test:lint:*",
    "test:lint:scss": "stylelint src/**/*.scss",
    "test:lint:js": "eslint --ext=js --ext=jsx src/",
    "test:unit": "react-scripts test --env=jsdom --coverage --colors",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "moment": "^2.24.0",
    "node-sass": "^4.11.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.8.3",
    "react-scripts": "^2.1.5",
    "reactstrap": "^7.1.0"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.16.0",
    "coveralls": "^3.0.3",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "npm-run-all": "^4.1.5",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.5.4"
  },
  "private": true,
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.jsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  }
}
